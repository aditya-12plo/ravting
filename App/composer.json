{
    "name": "laravel/laravel",
    "description": "E-MAPPING adalah Electronic Mitratel Approval Budgeting",
    "keywords": ["e-mapping", "mitratel","electronic mitratel approval budgeting"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "~2.0",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "creativeorange/gravatar": "~1.0",
        "cviebrock/eloquent-sluggable": ">=3.0.0-alpha",
        "fx3costa/laravelchartjs": "^2.3",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "~2.2",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "maatwebsite/excel": "~2.1.0",
        "roksta/toast": "^1.0",
		"tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
